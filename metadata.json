{
  "name": "codingfuture-cfdb",
  "version": "0.12.0",
  "author": "Andrey Galkin",
  "summary": "Universal multi-instance multi-type database setup tool with cgroup runtime isolation",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfdb",
  "project_page": "https://github.com/codingfuture/puppet-cfdb",
  "issues_url": "https://github.com/codingfuture/puppet-cfdb/issues",
  "tags": [
    "mysql",
    "postgresql",
    "elasticsearch",
    "db",
    "haproxy",
    "high-availability",
    "fail-over",
    "cluster",
    "replication",
    "backup",
    "restore",
    "repmgr",
    "percona",
    "xtradb"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "16.10",
        "17.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.7.0 <6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 0.12.0 < 0.13.0"
    },
    {
      "name": "codingfuture-cfsystem",
      "version_requirement": ">= 0.12.0 < 0.13.0"
    }
  ],
  "data_provider": "hiera"
}
